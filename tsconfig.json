{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": "./",
    "paths": {
      "@assets/*":[
        "./assets/*"
      ], 
      "@components/*":[
        "./src/components/*"
      ],
      "@routes/*":[
        "./src/routes/*"
      ],
      "@screens/*":[
        "./src/screens/*"
      ],
      "@data/*":[
        "./src/data/*"
      ],
      "@utils/*":[
        "./src/utils/*"
      ]
    }
  }
}
